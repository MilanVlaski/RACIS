package view;

import java.awt.Font;
import java.awt.GraphicsEnvironment;

public class Fonts {

	public static final Font TREE = new Font("Segoe UI Semilight", Font.PLAIN, 20);
	public static final Font COLUMN = new Font("Segoe UI", Font.PLAIN, 15);
	public static final Font INPUT_LABEL = new Font("Segoe UI", Font.ITALIC, 16);
	public static final Font INPUT_TEXT = new Font("Segoe UI", Font.PLAIN, 15);

	
	public static void main(String[] args) {
		for (String string : GraphicsEnvironment.getLocalGraphicsEnvironment().getAvailableFontFamilyNames()) {
			System.out.println(string);
		}
	}
}
