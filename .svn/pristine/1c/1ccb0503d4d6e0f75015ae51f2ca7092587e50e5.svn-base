package view;

import java.awt.Font;
import java.awt.GraphicsEnvironment;

public class Fonts {

	public static final Font TREE = new Font("Segoe UI Semilight", Font.PLAIN, 18);
	public static final Font COLUMN = new Font("Segoe UI", Font.BOLD, 14);
	
	public static void main(String[] args) {
		for (String string : GraphicsEnvironment.getLocalGraphicsEnvironment().getAvailableFontFamilyNames()) {
			System.out.println(string);
		}
	}
}
